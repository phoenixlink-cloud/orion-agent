# --- Orion Settings ---
ORION_MODE=safe                  # safe | pro | project
ORION_LOG_LEVEL=INFO             # DEBUG | INFO | WARN | ERROR
ORION_SANDBOX_ENABLED=true       # Run edits in sandbox first
ORION_PORT=8000                  # API server port
